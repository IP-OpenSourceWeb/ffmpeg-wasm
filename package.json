{
  "name": "ffmpeg-wasm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ffmpeg:clone": "git clone https://github.com/FFmpeg/FFmpeg.git ./ffmpeg",
    "ffmpeg:move": "node ./scripts/move-files.js --src=./ffmpeg --dest=./packages/ffmpeg",
    "ffmpeg:update": "node ./scripts/generate-projects.js --src=./packages/ffmpeg",
    "ffmpeg:all": "npm run ffmpeg:clone && npm run ffmpeg:move && npm run ffmpeg:update"
  },
  "private": true,
  "devDependencies": {
    "@types/node": "^20.3.1",
    "fs-extra": "^11.1.1",
    "nx": "16.0.1",
    "nx-cloud": "latest"
  },
  "workspaces": [
    "packages/*"
  ]
}

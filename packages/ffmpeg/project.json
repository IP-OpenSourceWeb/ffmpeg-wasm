{
  "name": "ffmpeg",
  "projectType": "library",
  "sourceRoot": "packages/ffmpeg",
  "tags": [],
  "targets": {
    "emmake": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "emconfigure ./configure --disable-x86asm --disable-stripping",
          "emmake make"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg/src",
        "color": true
      }
    },
    "make": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "./configure --disable-x86asm --disable-stripping",
          "make"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg/src",
        "color": true
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "emmake make install"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg/src",
        "color": true
      }
    },
    "repo:update": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run ffmpeg:repo:update-version",
          "nx run ffmpeg:repo:clone"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg",
        "color": true
      }
    },
    "repo:update-version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node ../repo.update-version.js"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg",
        "color": true
      }
    },
    "repo:clone": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node ../repo.clone.js"
        ],
        "parallel": false,
        "cwd": "packages/ffmpeg",
        "color": true
      }
    },
    "docker:install": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "docker run -t ffmpeg-wasm nx run ffmpeg:emmake && nx run ffmpeg:install"
        ],
        "parallel": false,
        "cwd": "",
        "color": true
      }
    },
    "docker:emmake": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "docker run -t ffmpeg-wasm nx run ffmpeg:emmake"
        ],
        "parallel": false,
        "cwd": "",
        "color": true
      }
    },
    "docker:make": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "docker run -t ffmpeg-wasm nx run ffmpeg:make"
        ],
        "parallel": false,
        "cwd": "",
        "color": true
      }
    }
  }
}